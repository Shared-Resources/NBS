select v.*, rowid from veiculos v
where v.chassi_completo = '';

select vk.*, rowid from veic_kardex vk
where vk.chassi_completo = '' order by DATA desc;

select vi.*, rowid from Veiculos_inventario vi
where vi.chassi_completo = '';

select vh.cod_modelo, vh.cod_produto, vh.*, rowid from veiculos_historico vh
where vh.chassi_resumido = '';
