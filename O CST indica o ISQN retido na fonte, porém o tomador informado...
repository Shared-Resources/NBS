select  controle, cst, cod_cst,
    base_iss,
    aliq_iss,
    valor_iss,
    ((base_iss*aliq_iss)/100) as novoValor,
    base_iss_retido,
    aliq_iss_sub,
    valor_iss_retido,
    ((base_iss_retido*aliq_iss_sub)/100) as valor_retido,
    total_servicos,total_nota,
    v.*,rowid
from vendas v
where
  controle in (3775)
  and serie = 'A1'
  and cod_empresa = 27
