select c.transferencia, c.transf_venda_cod_empresa, c.transf_venda_controle, c.transf_venda_serie, rowid 
from compra c 
where c.cod_controle = &controle
and c.cod_empresa = &cod_empresa


/* 
transferencia = S
transf_venda_cod_empresa = empresa saida transferencia;
transf_venda_controle = numero NF saida trasnferencia;
transf_venda_serie = serie NF saida trasnferencia;
*/
