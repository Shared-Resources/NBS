Devolução

select * from compra
where cod_controle= 411876 for update

--Mudar no compras o CFOP para CFOP de devolução InterEstadual

select * from compras_itens
where cod_controle= 411876 for update

--Mudar no compras items o CFOP para CFOP de devolução InterEstadual
